    "dotenv": "^16.4.7",
    "hono": "^4.6.5",
    "hono-rate-limiter": "^0.4.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "tough-cookie": "^6.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@vercel/node": "^3.0.0",
    "eslint": "^9.29.0",
