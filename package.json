{
  "name": "toonstream-api",
  "version": "1.0.0",
  "description": "A comprehensive RESTful API for scraping anime content from toonstream.love - Optimized for Vercel Serverless",
  "module": "api/index.js",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "dev": "bun --watch api/index.js",
